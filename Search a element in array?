#include<iostream>
using namespace std;
int main()
{
    int size,num,index;
    cout<<"Enter the size of array:";
    cin>>size;
    int a[size];
    
    for(int i=0; i<size; i++){
    cout<<"Enter the value in array:";
    cin>>a[i];}
    
    cout<<"Enter the value which are you finding in array:";
    cin>>num;
    for(int i=0; i<size; i++)
    {
        if(num==a[i])
        {
            index=i;
        }
    }
    cout<<"The index of value is :"<<index;
}
